<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.opeyemi.automatedhallpass.dbmodel.HallpassEntity">
        <table name="hallpass" schema="autopassdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="bookingDate">
                <column name="booking_date" nullable="false" precision="-1"/>
            </basic>
            <basic name="destination">
                <column name="destination" nullable="false"/>
            </basic>
            <basic name="purposeOfVisit">
                <column name="purpose_of_visit" nullable="false"/>
            </basic>
            <basic name="nameOfHost">
                <column name="name_of_host" nullable="false"/>
            </basic>
            <basic name="addressOfHost">
                <column name="address_of_host" nullable="false"/>
            </basic>
            <basic name="timeOut">
                <column name="time_out" nullable="false" precision="-1"/>
            </basic>
            <basic name="timeOfArrival">
                <column name="time_of_arrival" nullable="false" precision="-1"/>
            </basic>
            <basic name="signIn">
                <column name="sign_in" nullable="false"/>
            </basic>
            <basic name="hallAdminId">
                <column name="hall_admin_id" nullable="false" precision="11"/>
            </basic>
            <basic name="remarks">
                <column name="remarks" nullable="false"/>
            </basic>
            <basic name="studentId">
                <column name="student_id" nullable="false" precision="11"/>
            </basic>
            <many-to-one name="admindetailsByHallAdminId" optional="true"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.AdmindetailsEntity">
                <join-column name="hall_admin_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <many-to-one name="studentdetailsByStudentId" optional="true"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.StudentdetailsEntity">
                <join-column name="student_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
