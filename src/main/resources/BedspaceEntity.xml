<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.opeyemi.automatedhallpass.dbmodel.BedspaceEntity">
        <table name="bedspace" schema="autopassdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="roomId">
                <column name="room_id" precision="11"/>
            </basic>
            <basic name="bedSpace">
                <column name="bed_space" nullable="false"/>
            </basic>
            <many-to-one name="roomsByRoomId" optional="true"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.RoomsEntity">
                <join-column name="room_id" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="studentHallsById" mapped-by="bedspaceByAssignedBedId"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.StudentHallEntity"/>
        </attributes>
    </entity>
</entity-mappings>
