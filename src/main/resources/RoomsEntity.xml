<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.opeyemi.automatedhallpass.dbmodel.RoomsEntity">
        <table name="rooms" schema="autopassdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="roomNo">
                <column name="room_no" precision="11"/>
            </basic>
            <basic name="roomDescription">
                <column name="room_description" nullable="false"/>
            </basic>
            <basic name="noOfBedSpace">
                <column name="no_of_bed_space" precision="11"/>
            </basic>
            <basic name="hallId">
                <column name="hall_id" nullable="false" precision="11"/>
            </basic>
            <many-to-one name="hallByHallId" optional="true"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.HallEntity">
                <join-column name="hall_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="bedspacesById" mapped-by="roomsByRoomId"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.BedspaceEntity"/>
        </attributes>
    </entity>
</entity-mappings>
