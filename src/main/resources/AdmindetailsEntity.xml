<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.opeyemi.automatedhallpass.dbmodel.AdmindetailsEntity">
        <table name="admindetails" schema="autopassdb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="firstName">
                <column name="first_name"/>
            </basic>
            <basic name="lastName">
                <column name="last_name"/>
            </basic>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="emaillAddress">
                <column name="emaill_address"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <one-to-many name="hallpassesById" mapped-by="admindetailsByHallAdminId"
                         target-entity="com.opeyemi.automatedhallpass.dbmodel.HallpassEntity"/>
        </attributes>
    </entity>
</entity-mappings>
